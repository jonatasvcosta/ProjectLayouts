<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taqtile.myapplication">

    <application>
        <activity
            android:name=".activities.SampleMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.SampleActivitiesActivity"
            android:label="@string/activity_activities_label" />

        <activity
            android:name=".activities.SampleColorsActivity"
            android:label="@string/activity_colors_label" />

        <activity
            android:name=".activities.SampleControlsActivity"
            android:label="@string/activity_controls_label" />

        <activity
            android:name=".activities.SampleTypographyActivity"
            android:label="@string/activity_typography_label" />

    </application>
</manifest>
